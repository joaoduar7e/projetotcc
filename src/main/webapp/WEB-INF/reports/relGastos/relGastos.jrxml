	<queryString>
		<![CDATA[select c.nome,
       c.sobrenome,
       cr.dataemissao,
       cr.datavencimento,
       cr.pago,
       cr.valor,
       cr.observacao, cr.id
from contaspagar cr
         INNER JOIN cliente c on c.id = cr.cliente_id
where date(cr.dataemissao) between $P{dataI} and $P{dataF}
$P!{condicao}
order by cr.datavencimento DESC]]>
	</queryString>
