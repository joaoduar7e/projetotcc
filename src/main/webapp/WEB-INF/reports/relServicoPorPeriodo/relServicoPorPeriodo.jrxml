	<parameter name="condicao" class="java.lang.String"/>
	<queryString>
		<![CDATA[select s.descricao, coalesce(sum(i.quantidade), 0) as ttA, coalesce(sum(i2.quantidade), 0) as ttv,
       sum(coalesce(i.quantidade, 0) + i2.quantidade) as totalS
from servico s
         left join itensagendamento i on s.id = i.servico_id
         left join itensservico i2 on s.id = i2.servico_id
         left join vendas v on v.id = i2.vendas_id
where date(v.datavenda) between $P{dataI} and $P{dataF}
$P!{condicao}
group by s.descricao]]>
	</queryString>
