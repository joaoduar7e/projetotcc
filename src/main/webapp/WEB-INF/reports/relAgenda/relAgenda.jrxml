	<queryString>
		<![CDATA[select a.id, a.valortotals as valorServ,
       a.valortotal as valorTotal, c.nome as cliNome, c.sobrenome,
       c.telefone as telefone, a.dataagendamento, its.id, a.dataagendadada, a.finalizado

from agendamento a
         INNER join cliente c on c.id = a.cliente_id


         LEFT JOIN itensagendamento AS its ON a.id=its.id
         LEFT JOIN servico AS serv ON serv.id=its.id

where date(a.dataagendadada) between $P{dataI} and $P{dataF}
$P!{condicaoStatus}
GROUP BY a.id, c.nome, c.sobrenome, c.telefone, its.id
order by a.dataagendadada, a.id]]>
	</queryString>
