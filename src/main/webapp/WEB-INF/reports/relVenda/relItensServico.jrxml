	<queryString>
		<![CDATA[select s.id as sid, its.preco                as itspreco,
       its.quantidade           as itsqtd,
       s.descricao              as servnome,
       s.preco                  as servpreco, its.quantidade * its.preco as totalPs
from itensservico as its
         inner join servico as s on s.id = its.servico_id
         left join vendas as c on c.id = its.vendas_id
where its.vendas_id=$P{VENDA_ID}
order by s.descricao]]>
